{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "!node_modules",
        "src/**/*.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "src/collectionReducer.ts",
        "src/defaultCollection.ts",
        "src/defaultItemReducer.ts",
        "src/defaultItemSelector.ts",
        "src/defaultKeyPredicate.ts",
        "src/index.ts",
        "src/list/add.ts",
        "src/list/remove.ts",
        "src/types.ts"
    ]
}
